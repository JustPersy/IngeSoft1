<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.scalia.controllers.TuningLibraryController">
    <children>
        <VBox alignment="CENTER" spacing="10" AnchorPane.topAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
            <Label text="Biblioteca de Afinaciones" />
            <ComboBox fx:id="instrumentComboBox" promptText="Seleccione instrumento" />
            <ListView fx:id="tuningListView" prefHeight="200" />
            <Button text="Volver" onAction="#handleBackToMainMenu" />
        </VBox>
    </children>
</AnchorPane>
